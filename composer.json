{
    "name": "yaroslawww/nova-seo-entity",
    "description": "Related entity to manage SEO data.",
    "keywords": [
        "laravel",
        "nova",
        "seo"
    ],
    "homepage": "https://github.com/yaroslawww/nova-seo-entity",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.13",
        "artesaos/seotools": "^1.0",
        "yaroslawww/laravel-simple-image-manager": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "orchestra/testbench": "^8.3",
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-laravel": "^2.0",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "NovaSeoEntity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaSeoEntity\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaSeoEntity\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

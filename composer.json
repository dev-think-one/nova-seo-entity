{
    "name": "yaroslawww/nova-seo-entity",
    "description": "Related entity to manage SEO data.",
    "keywords": [
        "laravel",
        "nova",
        "seo"
    ],
    "homepage": "https://github.com/yaroslawww/nova-seo-entity",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "artesaos/seotools": "^0.20",
        "yaroslawww/laravel-simple-image-manager": "^2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "orchestra/testbench": "^6.21",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-laravel": "^1.5",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "NovaSeoEntity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaSeoEntity\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaSeoEntity\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
